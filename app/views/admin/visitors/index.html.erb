<h1>All Visitors</h1>
<head> 
<%= stylesheet_link_tag  'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>

</head>

<% @visitors.each do |visitor| %>
<p><%= visitor.fullname %></p>
<p><%= visitor.email %></p>
<p><%= time_ago visitor.created_at %></p>
<p><%= link_to 'Delete', admin_visitor_path(visitor), method: :delete, data: {confirm: ' Are you sure you would like to delete this user ?'} %></p>
<hr>


<% end %>
<%= paginate @visitors %>